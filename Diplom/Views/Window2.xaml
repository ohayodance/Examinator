<Window x:Class="Diplom.Views.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Diplom.Views"
        mc:Ignorable="d"
        Title="Приступить к тесту" Height="148" Width="258" Top="100" Left="100">
    <Grid Margin="0,0,0,2">
        <Label Content="Введите фамилию" HorizontalAlignment="Center" x:Name="ResponseTextBox" Margin="0,3,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Center" x:Name="RTextBox" Margin="0,29,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Content="OK" HorizontalAlignment="Center" Margin="0,63,0,0" VerticalAlignment="Top" Height="32" Width="61" Click="Button_Click"/>

    </Grid>
</Window>
